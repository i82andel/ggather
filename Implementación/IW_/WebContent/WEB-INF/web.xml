<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>IW_</display-name>
  <welcome-file-list>
    <welcome-file>mainMenu.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
        <servlet-name>RegistrarController</servlet-name>
        <servlet-class>com.mycompany.servlets.RegistrarController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrarController</servlet-name>
        <url-pattern>/RegistrarController</url-pattern>
    </servlet-mapping>
  <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.mycompany.servlets.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
   <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>com.mycompany.servlets.LogoutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>
   <servlet>
        <servlet-name>EditarPerfilController</servlet-name>
        <servlet-class>com.mycompany.servlets.EditarPerfilController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditarPerfilController</servlet-name>
        <url-pattern>/EditarPerfilController</url-pattern>
    </servlet-mapping>
</web-app>